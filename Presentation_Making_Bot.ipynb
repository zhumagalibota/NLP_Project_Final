      "source": [
        "TOKEN = \"-------------------------------------------------"\n",
        "bot = telebot.TeleBot(TOKEN)\n",
        "\n",
        "nlp = spacy.load(\"en_core_web_sm\")\n",
        "\n",
        "def extract_text_from_pdf(file_path):\n",
        "    try:\n",
        "        with pdfplumber.open(file_path) as pdf:\n",
        "            text = \"\"\n",
        "            for page in pdf.pages:\n",
        "                text += page.extract_text() or \"\"\n",
        "        return text.strip()\n",
        "    except Exception as e:\n",
        "        return f\"Error extracting text: {str(e)}\"\n",
        "\n",
        "def summarize_text(text, compression_ratio=0.3):\n",
        "    try:\n",
        "        doc = nlp(text)\n",
        "        stopwords = list(STOP_WORDS)\n",
        "        word_frequencies = {}\n",
        "\n",
        "        for word in doc:\n",
        "            if word.text.lower() not in stopwords and word.text.lower() not in punctuation:\n",
        "                word_frequencies[word.text.lower()] = word_frequencies.get(word.text.lower(), 0) + 1\n",
        "\n",
        "        max_frequency = max(word_frequencies.values(), default=1)\n",
        "        word_frequencies = {word: freq / max_frequency for word, freq in word_frequencies.items()}\n",
        "\n",
        "        sentence_scores = {}\n",
        "        for sentence in doc.sents:\n",
        "            for word in sentence:\n",
        "                if word.text.lower() in word_frequencies:\n",
        "                    sentence_scores[sentence] = sentence_scores.get(sentence, 0) + word_frequencies[word.text.lower()]\n",
        "\n",
        "        select_length = int(len(list(doc.sents)) * compression_ratio)\n",
        "        summarized_sentences = nlargest(select_length, sentence_scores, key=sentence_scores.get)\n",
        "        summary = ' '.join([sentence.text for sentence in summarized_sentences])\n",
        "        return summary\n",
        "    except Exception as e:\n",
        "        print(f\"Error summarizing text: {e}\")\n",
        "        return text\n",
        "\n",
        "def divide_into_slides(text, n):\n",
        "    words = text.split()\n",
        "    total_words = len(words)\n",
        "    words_per_slide = total_words // n\n",
        "\n",
        "    slides = []\n",
        "    start_idx = 0\n",
        "\n",
        "    for i in range(n):\n",
        "        if i == n - 1:\n",
        "            slide_words = words[start_idx:]\n",
        "        else:\n",
        "            end_idx = start_idx + words_per_slide\n",
        "\n",
        "            while end_idx < len(words) and not words[end_idx-1].endswith(('.', '!', '?')):\n",
        "                end_idx += 1\n",
        "            slide_words = words[start_idx:end_idx]\n",
        "            start_idx = end_idx\n",
        "\n",
        "        slide_text = ' '.join(slide_words)\n",
        "        if not slide_text.endswith(('.', '!', '?')):\n",
        "            slide_text += '.'\n",
        "        slides.append(slide_text)\n",
        "\n",
        "    return slides\n",
        "\n",
        "def format_text_box(shape):\n",
        "    shape.left = Inches(0.5)\n",
        "    shape.top = Inches(1.5)\n",
        "    shape.width = Inches(9)\n",
        "    shape.height = Inches(5)\n",
        "\n",
        "    text_frame = shape.text_frame\n",
        "    text_frame.word_wrap = True\n",
        "    text_frame.auto_size = True\n",
        "\n",
        "    p = text_frame.paragraphs[0]\n",
        "    p.alignment = PP_ALIGN.CENTER\n",
        "    p.line_spacing = 1.5\n",
        "\n",
        "    return text_frame\n",
        "\n",
        "def create_presentation(slides, title, name, date, font_color, bg_color, filename=\"presentation.pptx\"):\n",
        "    prs = Presentation()\n",
        "\n",
        "    prs.slide_width = Inches(13.333)\n",
        "    prs.slide_height = Inches(7.5)\n",
        "\n",
        "    # Title slide\n",
        "    first_slide = prs.slides.add_slide(prs.slide_layouts[0])\n",
        "    title_shape = first_slide.shapes.title\n",
        "    subtitle_shape = first_slide.placeholders[1]\n",
        "\n",
        "    title_shape.left = Inches(1)\n",
        "    title_shape.top = Inches(2)\n",
        "    title_shape.width = Inches(11.333)\n",
        "    title_shape.text = title\n",
        "    title_frame = title_shape.text_frame\n",
        "    title_frame.paragraphs[0].alignment = PP_ALIGN.CENTER\n",
        "    title_frame.paragraphs[0].font.size = Pt(44)\n",
        "    title_frame.paragraphs[0].font.bold = True\n",
        "\n",
        "    subtitle_shape.left = Inches(1)\n",
        "    subtitle_shape.top = Inches(4)\n",
        "    subtitle_shape.width = Inches(11.333)\n",
        "    subtitle_text = f\"Presented by: {name}\\nDate: {date}\"\n",
        "    subtitle_shape.text = subtitle_text\n",
        "    subtitle_frame = subtitle_shape.text_frame\n",
        "    for paragraph in subtitle_frame.paragraphs:\n",
        "        paragraph.alignment = PP_ALIGN.CENTER\n",
        "        paragraph.font.size = Pt(24)\n",
        "\n",
        "    title_frame.paragraphs[0].font.color.rgb = RGBColor(*font_color)\n",
        "    subtitle_frame.paragraphs[0].font.color.rgb = RGBColor(*font_color)\n",
        "    first_slide.background.fill.solid()\n",
        "    first_slide.background.fill.fore_color.rgb = RGBColor(*bg_color)\n",
        "\n",
        "    # Content slides\n",
        "    for slide_text in slides:\n",
        "        slide = prs.slides.add_slide(prs.slide_layouts[5])\n",
        "\n",
        "        left = Inches(1)\n",
        "        top = Inches(1)\n",
        "        width = Inches(11.333)\n",
        "        height = Inches(5.5)\n",
        "\n",
        "        textbox = slide.shapes.add_textbox(left, top, width, height)\n",
        "        text_frame = textbox.text_frame\n",
        "        text_frame.word_wrap = True\n",
        "\n",
        "        p = text_frame.paragraphs[0]\n",
        "        p.text = slide_text\n",
        "        p.alignment = PP_ALIGN.CENTER\n",
        "        p.font.size = Pt(28)\n",
        "        p.font.color.rgb = RGBColor(*font_color)\n",
        "        p.line_spacing = 1.5\n",
        "\n",
        "        slide.background.fill.solid()\n",
        "        slide.background.fill.fore_color.rgb = RGBColor(*bg_color)\n",
        "\n",
        "    # Last slide\n",
        "    last_slide = prs.slides.add_slide(prs.slide_layouts[0])\n",
        "    thank_you = last_slide.shapes.title\n",
        "    thank_you.left = Inches(1)\n",
        "    thank_you.top = Inches(2.5)\n",
        "    thank_you.width = Inches(11.333)\n",
        "    thank_you.text = \"THANK YOU\"\n",
        "\n",
        "    thank_frame = thank_you.text_frame\n",
        "    thank_frame.paragraphs[0].alignment = PP_ALIGN.CENTER\n",
        "    thank_frame.paragraphs[0].font.size = Pt(44)\n",
        "    thank_frame.paragraphs[0].font.bold = True\n",
        "    thank_frame.paragraphs[0].font.color.rgb = RGBColor(*font_color)\n",
        "\n",
        "    last_slide.background.fill.solid()\n",
        "    last_slide.background.fill.fore_color.rgb = RGBColor(*bg_color)\n",
        "\n",
        "    prs.save(filename)\n",
        "\n",
        "\n",
        "# Bot\n",
        "@bot.message_handler(commands=[\"start\"])\n",
        "def send_welcome(message):\n",
        "    bot.reply_to(\n",
        "        message,\n",
        "        \"Welcome! Send me a PDF file, and I'll extract text from it. You can then create a presentation from the extracted text!\"\n",
        "    )\n",
        "\n",
        "@bot.message_handler(content_types=[\"document\"])\n",
        "def handle_document(message):\n",
        "    try:\n",
        "        if message.document.mime_type != \"application/pdf\":\n",
        "            bot.reply_to(message, \"Please upload a valid PDF file.\")\n",
        "            return\n",
        "\n",
        "        file_info = bot.get_file(message.document.file_id)\n",
        "        file_path = f\"{message.document.file_id}.pdf\"\n",
        "        downloaded_file = bot.download_file(file_info.file_path)\n",
        "\n",
        "        with open(file_path, \"wb\") as f:\n",
        "            f.write(downloaded_file)\n",
        "\n",
        "        bot.reply_to(message, \"PDF received. Extracting text...\")\n",
        "\n",
        "        extracted_text = extract_text_from_pdf(file_path)\n",
        "        os.remove(file_path)\n",
        "\n",
        "        if not extracted_text:\n",
        "            bot.reply_to(message, \"Sorry, I couldn't extract any text from the PDF.\")\n",
        "            return\n",
        "\n",
        "        summarized_text = summarize_text(extracted_text)\n",
        "\n",
        "        bot.reply_to(message, \"Text extracted and summarized! Send me the title, your name, the number of slides, font color, and background color in RGB\")\n",
        "        bot.register_next_step_handler(message, handle_presentation_details, summarized_text)\n",
        "\n",
        "    except Exception as e:\n",
        "        bot.reply_to(message, f\"An error occurred: {str(e)}\")\n",
        "\n",
        "def handle_presentation_details(message, summarized_text):\n",
        "    try:\n",
        "        # title; name; number of slides; font color; background color\n",
        "        user_input = message.text.split(\"; \")\n",
        "\n",
        "        title = user_input[0]\n",
        "        name = user_input[1]\n",
        "        date = datetime.now().strftime(\"%Y-%m-%d\")\n",
        "        n = int(user_input[2])\n",
        "        font_color_input = user_input[3]\n",
        "        bg_color_input = user_input[4]\n",
        "\n",
        "        font_color = tuple(map(int, font_color_input.split(\", \")))\n",
        "        bg_color = tuple(map(int, bg_color_input.split(\", \")))\n",
        "\n",
        "        slides = divide_into_slides(summarized_text, n)\n",
        "        filename = f\"{message.chat.id}_presentation.pptx\"\n",
        "        create_presentation(slides, title, name, date, font_color, bg_color, filename)\n",
        "\n",
        "        with open(filename, \"rb\") as f:\n",
        "            bot.send_document(message.chat.id, f)\n",
        "\n",
        "        os.remove(filename)\n",
        "\n",
        "    except Exception as e:\n",
        "        bot.reply_to(message, f\"An error occurred: {str(e)}\")\n",
        "\n",
        "\n",
